
<h2>companies </h2>
<table id="company_list">
 <caption>company list</caption>
 <thead><tr><th>email_domain</th><th>name</th></tr></thead>
 <tbody>
[% FOREACH company = model.companies %]
<tr>
 <td>[% company.email_domain %]</td><td>[% company.name %]</td>
 <td>[<a href="[% SCRIPT_NAME %]/company/[% company.id_company %]">details</a>]</td>
</tr>
[% END %]
 </tbody>
</table>

 
